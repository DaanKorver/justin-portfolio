h1,\nh2,\nh3,\nh4,\nh5,\nh6,\np {\n    margin: 0;\n    padding: 0;\n}\n\n.hero__subtitle {\n    font-size: 2rem;\n    font-weight: 500;\n    margin: 0 0 .5em 0;\n\n    @include mq(768px) {\n        font-size: 1.4rem;\n    }\n}\n\n.hero__title {\n    font-size: 3.5rem;\n    font-weight: 700;\n    line-height: 1.3em;\n\n    @include mq(768px) {\n        font-size: 1.7rem;\n    }\n}\n\n.hero__text {\n    font-size: 1.15rem;\n    font-weight: 300;\n\n    @include mq(768px) {\n        font-size: .7rem;\n    }\n}","scss/components/_button.scss":".cta {\n    all: unset;\n    background-color: $tertiary;\n    color: $primary;\n    \n    border-radius: 50px;\n    font-size: \t.8rem;\n    font-weight: 550;\n    text-align: center;\n    padding: 1em 2em;\n    transition: .3s ease;\n\n    cursor: pointer;\n}","scss/utils/_variables.scss":"$primary: #080606;\n$secondairy: #A6A6A7;\n$tertiary: #EDEEEF;\n$overlayNav: #0e0e0f;","scss/base/_global.scss":"* {\n    scroll-behavior: smooth;\n}\n\nbody {\n    background-color: $primary;\n    color: $tertiary;\n    box-sizing: border-box;\n    padding: 0;\n    margin: 0;\n    font-family: 'Poppins', sans-serif;\n}","scss/utils/_mixins.scss":"@mixin mq($size) {\n    @media(max-width: $size) {\n        @content;\n    }\n}","scss/layout/_header.scss":"header {\n    padding: 2em;\n    display: flex;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n\n    .menu__open {\n        ul {\n            transform: translateX(0%);\n        }\n    }\n\n    .logo {\n        font-size: 1.5rem;\n        margin: auto auto 0 2.5em;\n        z-index: 100;\n\n        @include mq(768px) {\n            margin: 0;\n        }\n\n        @include mq(992px) {\n            margin: auto auto 0 0;\n        }\n    }\n\n    .nav {\n        padding: 2px;\n\n        a {\n            text-decoration: none;\n            color: $tertiary;\n        }\n\n        .nav__list {\n            position: relative;\n            z-index: 100;\n            margin: 0;\n            padding: 0;\n            list-style: none;\n            display: flex;\n            gap: 2.5em;\n            margin-left: 2.5em;\n\n            @include mq(768px) {\n                display: none;\n            }\n\n            li:first-child {\n                margin-left: auto;\n            }\n\n            li:last-child {\n                margin-right: 3em;\n                \n                .cta {\n                    color: $primary !important;\n                }\n\n                @include mq(992px) {\n                    margin: 0;\n                }\n            }\n        }\n\n        .hamburger-menu {\n            position: relative;\n            z-index: 100;\n            width: 30px;\n            height: 18px;\n            display: none;\n            margin-left: auto;\n            cursor: pointer;\n\n            @include mq(768px) {\n                display: block !important;\n            }\n\n            .hamburger-line {\n                width: 30px;\n                height: 2px;\n                background-color: white;\n                position: absolute;\n\n                &:first-child {\n                    top: 0;\n                    transition: top .3s .3s, transform .3s;\n                }\n\n                &:nth-child(2) {\n                    top: 50%;\n                    transform: translateY(-50%);\n                    transition: opacity .3s .3s;\n                }\n\n                &:last-child {\n                    bottom: 0;\n                    transition: bottom .3s .3s, transform .3s;\n                }\n            }\n        }\n\n        .hamburger__open {\n            .hamburger-line {\n                &:first-child {\n                    top: 50%;\n                    transform: translateY(-50%) rotate(45deg);\n                    transition: top .2s, transform .2s .2s;\n                }\n\n                &:nth-child(2) {\n                    opacity: 0;\n                    transition: opacity 0s .2s;\n                }\n\n                &:last-child {\n                    bottom: 50%;\n                    transform: translateY(50%) rotate(-45deg);\n                    transition: bottom .3s, transform .2s .2s;\n                }\n            }\n        }\n\n        .overlay {\n            position: fixed;\n            top: 0;\n            left: 0;\n            z-index: 99;\n\n            width: 100%;\n            height: 100vh;\n            background: $overlayNav;\n            \n            display: flex;\n            align-items: center;\n            transform: translateX(-100%);\n            transition: .3s ease-in-out;\n\n\n            &__list {\n                list-style: none;\n\n                li {\n                    font-size: 2.5em;\n                    margin-left: .5em;\n                }\n            }\n        }\n\n        .overlay.active {\n            transform: translateX(0%);\n        }\n    }\n}","scss/layout/_home.scss":"#home {\n    height: 80vh;\n    margin: 0;\n    padding: 0;\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    align-items: center;\n    text-align: center;\n\n    .sub-hero-container {\n        display: flex;\n        justify-content: center;\n        align-items: center;\n\n        .hero__subtitle {\n            margin: 0 .5em;\n        }\n\n        img {\n            max-width: 30px;\n        }\n    }\n\n    .hero__title {\n        margin-top: .4em;\n    }\n\n\n    .hero__text {\n        margin-top: 1em;\n    }\n\n    .cta {\n        margin-top: 1.5em;\n    }\n}"},"lineCount":null}},"error":null,"hash":"2748c4685a6c6e53e858dfef8e700abd","cacheData":{"env":{}}}